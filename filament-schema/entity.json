{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "自定义实体配置",
    "description": "用于定义 Filament 数据包中自定义实体的 JSON 架构,支持实体的属性、行为、生成规则等配置",
    "type": "object",
    "required": [
        "id",
        "entity_type"
    ],
    "properties": {
        "id": {
            "description": "实体的唯一标识符,格式为 '<namespace>:<entityname>',用于在游戏中引用该实体",
            "type": "string",
            "pattern": "^[a-z0-9_]+:[a-z0-9_/]+$",
            "examples": [
                "mynamespace:penguin",
                "mynamespace:unfriendly_golem"
            ]
        },
        "entity_tags": {
            "description": "实体的标签列表,用于分类或应用特定行为（如水中呼吸）",
            "type": "array",
            "items": {
                "type": "string"
            },
            "examples": [
                [
                    "can_breathe_underwater",
                    "is_hostile"
                ]
            ]
        },
        "entity_type": {
            "description": "实体类型,决定实体的基础行为和渲染方式",
            "type": "string",
            "enum": [
                "block_display",
                "item_display",
                "text_display",
                "interaction",
                "custom",
                "axolotl",
                "iron_golem",
                "zombie",
                "skeleton",
                "pig",
                "cow"
            ],
            "examples": [
                "block_display",
                "iron_golem"
            ]
        },
        "translations": {
            "description": "实体名称的翻译,支持多种语言,键为语言代码,值为显示名称",
            "type": "object",
            "additionalProperties": {
                "type": "string"
            },
            "examples": [
                {
                    "en_us": "Angry Penguin",
                    "zh_cn": "愤怒企鹅"
                }
            ]
        },
        "animation": {
            "description": "实体的模型和动画配置,用于自定义外观和动作",
            "type": "object",
            "properties": {
                "model": {
                    "type": "string",
                    "description": "实体模型的标识符,格式为 '<namespace>:<modelname>',引用 Blockbench 模型",
                    "pattern": "^[a-z0-9_]+:[a-z0-9_/]+$",
                    "examples": [
                        "mynamespace:penguin"
                    ]
                },
                "walk_animation": {
                    "type": "string",
                    "description": "行走动画的标识符,引用模型中的动画名称",
                    "examples": [
                        "walk"
                    ]
                },
                "idle_animation": {
                    "type": "string",
                    "description": "空闲动画的标识符,引用模型中的动画名称",
                    "examples": [
                        "idle"
                    ]
                },
                "attack_animation": {
                    "type": "string",
                    "description": "攻击动画的标识符（可选）,引用模型中的动画名称",
                    "examples": [
                        "attack"
                    ]
                }
            },
            "required": [
                "model"
            ]
        },
        "properties": {
            "description": "实体的基本属性,控制其物理特性、分类、声音等",
            "type": "object",
            "properties": {
                "size": {
                    "type": "array",
                    "items": {
                        "type": "number",
                        "minimum": 0
                    },
                    "description": "实体碰撞箱的尺寸,格式为 [宽度, 高度],单位为方块",
                    "minItems": 2,
                    "maxItems": 2,
                    "examples": [
                        [
                            0.5,
                            0.6
                        ],
                        [
                            0.5,
                            1.8
                        ]
                    ]
                },
                "category": {
                    "type": "string",
                    "description": "实体所属的类别,影响生成和行为（如怪物仅在夜间或黑暗处生成）",
                    "enum": [
                        "monster",
                        "creature",
                        "ambient",
                        "water_creature",
                        "water_ambient",
                        "misc"
                    ],
                    "examples": [
                        "monster",
                        "creature"
                    ]
                },
                "xp_reward": {
                    "type": "integer",
                    "description": "实体死亡时掉落的经验值",
                    "minimum": 0,
                    "default": 5,
                    "examples": [
                        5,
                        15
                    ]
                },
                "is_sun_sensitive": {
                    "type": "boolean",
                    "description": "是否对阳光敏感,启用后实体在阳光下会燃烧（如僵尸）",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "should_despawn_in_peaceful": {
                    "type": "boolean",
                    "description": "是否在和平模式下消失,通常用于敌对实体",
                    "default": true,
                    "examples": [
                        true,
                        false
                    ]
                },
                "invulnerable": {
                    "type": "boolean",
                    "description": "是否无敌,启用后实体无法受到任何伤害",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "fire_immune": {
                    "type": "boolean",
                    "description": "是否免疫火焰和熔岩伤害",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "no_save": {
                    "type": "boolean",
                    "description": "是否不保存到世界存档,启用后实体在世界保存时不会持久化",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "no_summon": {
                    "type": "boolean",
                    "description": "是否禁止通过命令或刷怪蛋召唤",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "no_physics": {
                    "type": "boolean",
                    "description": "是否忽略物理效果（如重力、碰撞）",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "can_use_portal": {
                    "type": "boolean",
                    "description": "是否可以使用下界传送门",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "can_be_leashed": {
                    "type": "boolean",
                    "description": "是否可以被拴绳牵引",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "despawn_when_far_away": {
                    "type": "boolean",
                    "description": "是否在远离玩家时消失,通常用于非持久化实体",
                    "default": false,
                    "examples": [
                        true,
                        false
                    ]
                },
                "offspring": {
                    "type": "string",
                    "description": "繁殖产生的后代实体类型,格式为 '<namespace>:<entityname>'",
                    "examples": [
                        "minecraft:pig"
                    ]
                },
                "food": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "实体可以吃的物品列表,用于诱导或繁殖",
                    "examples": [
                        [
                            "minecraft:sugar_cane"
                        ]
                    ]
                },
                "ambient_sound_interval": {
                    "type": "integer",
                    "description": "环境音效播放的间隔（以游戏刻为单位,20刻=1秒）",
                    "minimum": 0,
                    "default": 80,
                    "examples": [
                        80
                    ]
                },
                "sounds": {
                    "type": "object",
                    "description": "实体音效配置,定义各种状态下的音效",
                    "properties": {
                        "ambient": {
                            "type": "string",
                            "description": "环境音效,实体在正常状态下随机播放"
                        },
                        "hurt": {
                            "type": "string",
                            "description": "受伤音效,实体受到伤害时播放"
                        },
                        "death": {
                            "type": "string",
                            "description": "死亡音效,实体死亡时播放"
                        },
                        "swim_splash": {
                            "type": "string",
                            "description": "游泳溅水音效,实体进入水面时播放"
                        },
                        "swim": {
                            "type": "string",
                            "description": "游泳音效,实体在水中移动时播放"
                        },
                        "fall": {
                            "type": "object",
                            "description": "摔落音效,根据摔落高度分为小摔和大摔",
                            "properties": {
                                "small": {
                                    "type": "string",
                                    "description": "小型摔落音效（低高度）"
                                },
                                "big": {
                                    "type": "string",
                                    "description": "大型摔落音效（高高度）"
                                }
                            }
                        }
                    },
                    "examples": [
                        {
                            "ambient": "entity.armadillo.ambient",
                            "hurt": "entity.armadillo.hurt",
                            "death": "entity.armadillo.death",
                            "swim_splash": "entity.generic.splash",
                            "swim": "entity.generic.swim",
                            "fall": {
                                "small": "entity.generic.small_fall",
                                "big": "entity.generic.big_fall"
                            }
                        }
                    ]
                }
            }
        },
        "goals": {
            "description": "实体的 AI 行为目标列表,控制其行为逻辑（如攻击、漫步等）",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "description": "目标类型,定义具体的 AI 行为",
                        "enum": [
                            "float_goal",
                            "look_at_mob_goal",
                            "melee_attack_goal",
                            "move_through_village_goal",
                            "random_look_around_goal",
                            "remove_block_goal",
                            "water_avoiding_random_stroll_goal",
                            "defend_village_goal",
                            "hurt_by_target_goal",
                            "nearest_attackable_target_goal",
                            "break_door_goal",
                            "breed_goal",
                            "eat_block_goal",
                            "flee_sun_goal",
                            "follow_boat_goal",
                            "follow_mob_goal",
                            "follow_parent_goal",
                            "move_towards_target_goal",
                            "open_door_goal",
                            "panic_goal",
                            "random_stroll_goal",
                            "random_swimming_goal",
                            "stroll_through_village_goal",
                            "tempt_goal",
                            "try_find_water_goal"
                        ]
                    },
                    "priority": {
                        "type": "integer",
                        "description": "目标的优先级,值越低越优先执行",
                        "minimum": 0,
                        "examples": [
                            1,
                            2,
                            8
                        ]
                    },
                    "items": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "与目标相关的物品列表（如诱导目标的物品）",
                        "examples": [
                            [
                                "minecraft:sugar_cane"
                            ]
                        ]
                    },
                    "block": {
                        "type": "string",
                        "description": "与目标相关的方块（如移除方块目标的方块类型）",
                        "examples": [
                            "minecraft:turtle_egg"
                        ]
                    },
                    "speed_modifier": {
                        "type": "number",
                        "description": "移动速度的倍率,影响目标执行时的速度",
                        "minimum": 0,
                        "default": 1.0,
                        "examples": [
                            1.0,
                            0.8
                        ]
                    },
                    "vertical_search_range": {
                        "type": "integer",
                        "description": "垂直搜索范围,用于如移除方块目标的搜索高度",
                        "minimum": 0,
                        "examples": [
                            3
                        ]
                    },
                    "target": {
                        "type": "string",
                        "description": "目标实体类型（如攻击或观察的目标）",
                        "examples": [
                            "minecraft:player",
                            "minecraft:villager"
                        ]
                    },
                    "look_distance": {
                        "type": "number",
                        "description": "观察距离,实体能看到目标的最大距离",
                        "minimum": 0,
                        "examples": [
                            8.0
                        ]
                    },
                    "probability": {
                        "type": "number",
                        "description": "行为触发的概率,范围从 0.0 到 1.0",
                        "minimum": 0,
                        "maximum": 1,
                        "examples": [
                            0.02,
                            0.001
                        ]
                    },
                    "following_target_even_if_not_seen": {
                        "type": "boolean",
                        "description": "是否在看不见目标时仍继续追随",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "only_at_night": {
                        "type": "boolean",
                        "description": "是否仅在夜间执行该目标",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "distance_to_poi": {
                        "type": "integer",
                        "description": "到兴趣点的最大距离（如村庄导航）",
                        "minimum": 0,
                        "examples": [
                            8
                        ]
                    },
                    "can_deal_with_doors": {
                        "type": "boolean",
                        "description": "是否能与门互动（如打开或破坏门）",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "alert_others": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "受到攻击时通知的其他实体类型",
                        "examples": [
                            [
                                "minecraft:zombified_piglin"
                            ]
                        ]
                    },
                    "must_see": {
                        "type": "boolean",
                        "description": "是否必须看到目标才能执行行为",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "must_reach": {
                        "type": "boolean",
                        "description": "是否必须能到达目标位置",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "random_interval": {
                        "type": "integer",
                        "description": "目标重新评估的随机间隔（以刻为单位）",
                        "minimum": 0,
                        "examples": [
                            20
                        ]
                    },
                    "ignore_baby": {
                        "type": "boolean",
                        "description": "是否忽略幼年版本的目标实体",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "ignore_in_water": {
                        "type": "boolean",
                        "description": "是否忽略在水中的目标实体",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "door_break_time": {
                        "type": "integer",
                        "description": "破坏门所需的时间（以刻为单位）",
                        "minimum": 0,
                        "default": 240,
                        "examples": [
                            240
                        ]
                    },
                    "valid_difficulties": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "peaceful",
                                "easy",
                                "normal",
                                "hard"
                            ]
                        },
                        "description": "允许破坏门的游戏难度",
                        "examples": [
                            [
                                "normal",
                                "hard"
                            ]
                        ]
                    },
                    "can_scare": {
                        "type": "boolean",
                        "description": "诱导目标是否可能因惊吓而逃跑",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "item_tags": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "诱导目标的物品标签列表",
                        "examples": [
                            [
                                "minecraft:flowers"
                            ]
                        ]
                    },
                    "stop_distance": {
                        "type": "number",
                        "description": "跟随目标时停止跟随的最大距离",
                        "minimum": 0,
                        "examples": [
                            2.0
                        ]
                    },
                    "area_size": {
                        "type": "integer",
                        "description": "跟随目标的区域大小（待文档补充）",
                        "minimum": 0
                    },
                    "close_door": {
                        "type": "boolean",
                        "description": "打开门后是否关闭门",
                        "default": true,
                        "examples": [
                            true,
                            false
                        ]
                    },
                    "damage_type": {
                        "type": "string",
                        "description": "触发恐慌目标的伤害类型",
                        "examples": [
                            "minecraft:panic_causes"
                        ]
                    },
                    "interval": {
                        "type": "integer",
                        "description": "行为之间的间隔时间（以刻为单位）",
                        "minimum": 0,
                        "default": 240,
                        "examples": [
                            100,
                            240
                        ]
                    },
                    "check_no_action_time": {
                        "type": "boolean",
                        "description": "是否根据空闲时间触发漫步行为",
                        "default": false,
                        "examples": [
                            true,
                            false
                        ]
                    }
                },
                "required": [
                    "type",
                    "priority"
                ]
            }
        },
        "attributes": {
            "description": "实体的属性,控制其基础统计数据（如速度、攻击力等）",
            "type": "object",
            "additionalProperties": {
                "type": "number",
                "minimum": 0
            },
            "properties": {
                "minecraft:health": {
                    "description": "实体的生命值",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1024
                },
                "minecraft:movement": {
                    "description": "实体的移动速度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1.0
                },
                "minecraft:follow_range": {
                    "description": "实体主动追踪目标的距离",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 2048
                },
                "minecraft:attack_damage": {
                    "description": "实体的攻击伤害",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 2048
                },
                "minecraft:knockback_resistance": {
                    "description": "实体的击退抗性",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 1.0
                },
                "minecraft:absorption": {
                    "description": "实体的吸收量",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1024
                },
                "minecraft:lava_movement": {
                    "description": "实体在熔岩中的移动速度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1.0
                },
                "minecraft:underwater_movement": {
                    "description": "实体在水中的移动速度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1.0
                },
                "minecraft:jump_strength": {
                    "description": "实体的跳跃强度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 2.0
                },
                "minecraft:fall_damage": {
                    "description": "实体受到的跌落伤害倍率",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 100.0
                },
                "minecraft:luck": {
                    "description": "实体的幸运值",
                    "type": "number",
                    "minimum": -1024,
                    "maximum": 1024
                },
                "minecraft:horse.jump_strength": {
                    "description": "马的跳跃强度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 2.0
                },
                "minecraft:zombie.spawn_reinforcements": {
                    "description": "僵尸召唤增援的几率",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 1.0
                },
                "minecraft:attack_speed": {
                    "description": "实体的攻击速度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1024
                },
                "minecraft:armor": {
                    "description": "实体的护甲值",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 30
                },
                "minecraft:armor_toughness": {
                    "description": "实体的护甲韧性",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 20
                },
                "minecraft:attack_range": {
                    "description": "实体的攻击范围",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 64
                },
                "minecraft:step_height": {
                    "description": "实体的步高",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 10
                },
                "minecraft:tempt_range": {
                    "description": "实体被诱导的范围",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 2048
                },
                "minecraft:generic.max_absorption": {
                    "description": "实体的最大吸收量",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1024
                },
                "minecraft:generic.burning_time": {
                    "description": "实体燃烧的时间（以秒为单位）",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1000
                },
                "minecraft:generic.explosion_knockback_resistance": {
                    "description": "实体对爆炸击退的抗性",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 1.0
                },
                "minecraft:generic.movement_efficiency": {
                    "description": "实体在不同地形上的移动效率",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 1.0
                },
                "minecraft:generic.oxygen_bonus": {
                    "description": "实体在水下呼吸时间的加成",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1000
                },
                "minecraft:generic.water_movement_efficiency": {
                    "description": "实体在水中的移动效率",
                    "type": "number",
                    "minimum": 0.0,
                    "maximum": 1.0
                },
                "minecraft:generic.scale": {
                    "description": "实体的模型缩放比例",
                    "type": "number",
                    "minimum": 0.0625,
                    "maximum": 16.0
                },
                "minecraft:generic.block_interaction_range": {
                    "description": "实体与方块交互的范围",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 64
                },
                "minecraft:generic.entity_interaction_range": {
                    "description": "实体与其他实体交互的范围",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 64
                },
                "minecraft:generic.block_break_speed": {
                    "description": "实体破坏方块的速度",
                    "type": "number",
                    "minimum": 0,
                    "maximum": 1000
                },
                "minecraft:generic.mining_speed": {
